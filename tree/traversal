   question link - https://leetcode.com/problems/sum-of-left-leaves/
   
     int sumtt(TreeNode* root,int curr)
     {
       if(root==NULL)
       {   
          
           return 0;
       }   
          curr=curr*10+root->val;
          if(root->left==NULL&&root->right==NULL)
              return curr;
             
          return  sumtt(root->left,curr)+sumtt(root->right,curr);
            
     
     
     }
    
    
    int sumNumbers(TreeNode* root) {
      
         sumtt(root,0);
     
        return sumtt(root,0);
    }
